{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{if .Root}}Admin: {{end}} {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#185F7B",
          "template": "\ue0c5"
        },
        {
          "type": "text",
          "style": "powerline",
          "powerline_symbol": "\ue0c4",
          "foreground": "#185F7B",
          "background": "#185F7B"
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#185F7B",
          "trailing_diamond": "\ue0b0",
          "template": "{{ .Path }} ",
          "properties": {
            "home_icon": "\ue617  ",
            "style": "mixed"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#6f42c1",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "text",
          "style": "plain",
          "leading_diamond": "<transparent,#49404f>\ue0b0</>",
          "trailing_diamond": "\ue0b0"
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#4caf50",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "template": " {{ if gt .Code 0 }}\uf421{{ else }}\uf469 â™¥{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "time",
          "style": "diamond",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#2B2B2B",
          "leading_diamond": "\ue0c5",
          "trailing_diamond": "\ue0c4",
          "template": " {{ .CurrentDate | date .Format }}<#ffffff> \ue641 </>",
          "properties": {
            "time_format": "02 Jan 06 15:04:00 MST"
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#2B2B2B",
          "leading_diamond": "\ue0c5",
          "trailing_diamond": "\ue0c4",
          "template": " {{ .FormattedMs }}<#ffffff> \uf252 </>",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#2B2B2B",
          "template": "\ue0c5"
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\ue0c4",
          "foreground": "#1CA6A3",
          "background": "#2B2B2B",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#FBD951",
          "background": "#2B2B2B",
          "template": "\uf0e7 "
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#DC291E",
          "template": " {{ .UserName }} "
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#666666",
          "background": "#000000",
          "template": "\uf489 {{ .Name }}"
        }
      ]
    }
  ]
}